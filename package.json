{
   "name": "@heatboom/heat-auction-sdk",
 "version": "1.1.1",
 "description": "Heat Auction System SDK - Solana auction bidding functionality",
 "main": "dist/index.js",
 "types": "dist/index.d.ts",
   "files": [
    "dist/**/*",
    "src/**/*",
    ".env.example"
  ],
   "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "example:bid": "ts-node src/scripts/example-place-bid.ts",
