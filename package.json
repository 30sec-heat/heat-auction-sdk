  },

   "dependencies": {
    "@coral-xyz/anchor": "^0.31.1",
    "@metaplex-foundation/mpl-token-metadata": "^3.2.1",
    "@solana/spl-token": "^0.4.13",
    "@solana/web3.js": "^1.95.3",
    "dotenv": "^16.3.1"
  },
 "devDependencies": {
   "@types/node": "^20.10.0",
   "ts-node": "^10.9.2",
   "typescript": "^5.6.3"
 },
 "peerDependencies": {
   "@coral-xyz/anchor": "^0.31.1",
   "@solana/web3.js": "^1.95.3"
 }
